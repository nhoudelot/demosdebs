#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

%:
	dh $@ 

override_dh_strip:
	dh_strip -Xkuantum2-linux

override_dh_install:
	dh_install
	pandoc -s -t man -f markdown debian/kuantumakelerator2-by-hno3.6.md -o debian/kuantumakelerator2-by-hno3.6
