#!/usr/bin/make -f

# dh_dwz on buster don't like when there is no .debug_info section
ifneq ($(shell sed -n "s/^VERSION_CODENAME=//p" /etc/os-release),buster)
    RUN_DHDWZ=dh_dwz 
endif

%:
	dh $@

override_dh_auto_install:
	find . -name ".DS_Store" -delete
	dh_auto_install

override_dh_dwz:
	$(RUN_DHDWZ)
