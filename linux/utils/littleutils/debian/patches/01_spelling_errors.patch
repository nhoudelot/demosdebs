       (void) fclose (fp);
       exit (3);
@@ -284,7 +284,7 @@
     }
   if (setjmp (png_jmpbuf (write_png_ptr)))
     {
-      fprintf (stderr, "bizzare pngstrip error: unknown problem on write\n");
