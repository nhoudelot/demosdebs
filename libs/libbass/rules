#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1
DEB_HOST_MULTIARCH ?= $(shell dpkg-architecture -qDEB_HOST_MULTIARCH)

%:
	dh $@ 

override_dh_auto_install:
	dh_auto_install
	mkdir -p debian/libbass/usr/lib/${DEB_HOST_MULTIARCH} 
	install ${DEB_HOST_MULTIARCH}/libbass.so debian/libbass/usr/lib/${DEB_HOST_MULTIARCH} 
