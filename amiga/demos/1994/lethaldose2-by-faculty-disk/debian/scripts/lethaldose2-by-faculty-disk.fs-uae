# FS-UAE configuration for lethaldose2-by-faculty-disk

[config]
scanlines = 1
fullscreen = 0
window_width = 720
window_height = 576
keep_aspect = 1
kickstarts_dir = /usr/share/amiga/kickstarts
kickstart_file = aros-amiga-m68k-rom.bin
kickstart_ext_file = aros-amiga-m68k-ext.bin

[fs-uae]
amiga_model = A1200
floppy_drive_0 = /usr/share/lethaldose2-by-faculty-disk/lethaldose2-by-faculty-disk1.adz
floppy_image_0 = /usr/share/lethaldose2-by-faculty-disk/lethaldose2-by-faculty-disk1.adz
floppy_image_1 = /usr/share/lethaldose2-by-faculty-disk/lethaldose2-by-faculty-disk2.adz
