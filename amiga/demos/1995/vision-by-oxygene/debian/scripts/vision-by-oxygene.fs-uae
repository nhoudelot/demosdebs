# FS-UAE configuration for vision-by-oxygene

[config]
scanlines = 1
fullscreen = 0
window_width = 720
window_height = 576
keep_aspect = 1
kickstarts_file = internal

[fs-uae]
amiga_model = A1200/020
hard_drive_0 = /usr/share/amiga/whdloadharddisk/whdloadharddisk.hdf
hard_drive_1 = /usr/share/vision-by-oxygene/DH1.zip
hard_drive_4 = ~/.local/share/WHDSaves/
hard_drive_4_label = WHDSaves
fast_memory = 4096
